{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "no-any": true,
    "prefer-object-spread": true,
    "member-access": false,
    "jsx-no-lambda": false,
    "interface-name": false,
    "no-empty": true,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": true,
    "no-var-requires": true
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
